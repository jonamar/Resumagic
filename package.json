{
  "name": "resumagic-app",
  "version": "1.0.0",
  "description": "Professional resume and cover letter generator with intelligent keyword analysis",
  "main": "generate-resume.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathIgnorePatterns=integration",
    "test:integration": "jest __tests__/integration",
    "test:all": "npm run test && npm run test:python",
    "test:python": "cd services/keyword-analysis && python run_tests.py",
    "lint": "eslint .",
    "lint:errors": "eslint . --config .eslintrc.error-handling.js",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "docx": "^9.5.1",
    "gray-matter": "^4.0.3",
    "jszip": "^3.10.1",
    "marked": "^16.0.0"
  },
  "devDependencies": {
    "jest": "^29.7.0"
  }
}
